#9.d
import datetime as dt

class Avtale:
    def __init__(self, avtale, sted, starttidspunkt, varighet):
        self.avtale = avtale
        self.sted = sted
        self.starttidspunkt = starttidspunkt
        self.starttidspunkt = starttidspunkt
        self.varighet = int(varighet)
    
        
#9.e
    def __str__(self):
        return f"Avtalen er: {self.avtale}, {self.sted}, {self.starttidspunkt}, {self.varighet}"

avtalen = Avtale("et mÃ¸te", "uis", dt.datetime.now(), 30)
print(avtalen)

