import datetime as dt

class Avtale:
    def __init__(self, avtale, sted, starttidspunkt, varighet):
        self.sted = sted
        self.starttidspunkt = starttidspunkt
        self.starttidspunkt = starttidspunkt
        self.varighet = int(varighet)
    
        
#9.e
    def __str__(self):
        return f"Avtale: {self.sted}, {self.starttidspunkt}, {self.varighet}"

avtale = Avtale("mÃ¸te", "uis", dt.datetime.now(), 30)
print(avtale)
